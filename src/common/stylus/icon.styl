@font-face
  font-family: 'seller-icon';
  src:  url('~/src/common/fonts/seller-icon.eot?u22fxf');
  src:  url('~/src/common/fonts/seller-icon.eot?u22fxf#iefix') format('embedded-opentype'),
    url('~/src/common/fonts/seller-icon.ttf?u22fxf') format('truetype'),
    url('~/src/common/fonts/seller-icon.woff?u22fxf') format('woff'),
    url('~/src/common/fonts/seller-icon.svg?u22fxf#seller-icon') format('svg');
  font-weight: normal;
  font-style: normal;

[class^="icon-"], [class*=" icon-"]
  font-family: 'seller-icon' !important
  speak: none
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1

  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale

